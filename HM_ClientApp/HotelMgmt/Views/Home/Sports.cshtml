@using HotelMgmt.Models
@{
    ViewBag.Title = "Sports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/>
<br/>
<br/>
<h2>Sports at our location</h2>
<div id="DisplayData">
    
        <table border=1 style="border:thin;padding:2px;">
            <tr><td><b>Sport Number</b></td><td><b>Sport Name</b></td></tr>
            @{var data = ViewBag.Message;
            for (int i = 0; i < data.Split(new char[] { ',' }).Length; i++)
            {
            <tr><td> @i</td><td> @data.Split(new char[] { ',' })[i]</td></tr>
            }
            }
        </table>

</div>
